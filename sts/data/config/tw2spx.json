{
  "name": "Traditional Chinese (Taiwan standard) to Simplified Chinese (with phrases and extended simplification)",
  "requires": [
    "_default"
  ],
  "dicts": [
    {
      "file": "../dictionary/tw2spx.tlist",
      "auto_space": true,
      "mode": "join",
