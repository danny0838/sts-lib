{
  "name": "Traditional Chinese (Taiwan standard) to Simplified Chinese (with phrases)",
  "requires": [
    "_default.json"
  ],
  "dicts": [
    {
      "file": "../dictionary/tw2sp.tlist",
      "auto_space": true,
      "mode": "join",
      "src": [
        {
          "mode": "load",
          "src": [
            "../dictionary/TWPhrasesRev.list",
            "../../opencc/dictionary/TWVariantsRevPhrases.txt",
            "../dictionary/TWVariantsRev.list"
          ]
        },
        {
          "mode": "load",
          "src": [
            "../../opencc/dictionary/TSPhrases.txt",
            "../../opencc/dictionary/TSCharacters.txt"
          ]
        }
      ]
    }
  ]
}
